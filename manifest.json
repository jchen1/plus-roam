{
  "background": { "scripts": ["background.js"] },
  "browser_action": {
    "default_icon": "icon-128.png",
    "default_title": "+Roam"
  },
  "name": "+Roam",
  "description": "Save snippets to Roam Research",
  "homepage_url": "https://github.com/jchen1/plus-roam",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "permissions": ["activeTab", "contextMenus"],
  "version": "0.3",
  "manifest_version": 2,
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+E"
      }
    }
  }
}
